<div class="col-md-4 beer-card">
  <div class="card">
    <img src="{{this.image_url}}" class="card-img-top" alt="{{this.name}}">
    <div class="card-body">
      <h5 class="card-title"><a href="/{{this.id}}">{{this.name}}</a></h5>
      <p class="card-text">{{this.description}}</p>
      <p class="card-text"><small class="text-muted">{{this.tagline}}</small></p>
    </div>
  </div>

</div>


